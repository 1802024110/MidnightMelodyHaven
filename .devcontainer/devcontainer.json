{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [3000],
  "name": "深夜月馆",
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter",
        "nash.awesome-flutter-snippets",
        "circlecodesolution.ccs-flutter-color",
        "formulahendry.code-runner"
      ]
    }
  },
  // 执行flutter pub get，不然可能导致缺少包报错
  "postCreateCommand": "flutter pub get"
}
